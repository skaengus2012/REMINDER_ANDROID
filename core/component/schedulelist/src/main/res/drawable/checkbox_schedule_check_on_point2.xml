<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="@dimen/schedule_check_size"
                android:height="@dimen/schedule_check_size" />
            <stroke
                android:width="@dimen/schedule_check_external_dither"
                android:color="?point_2" />
        </shape>
    </item>

    <item
        android:bottom="4.5dp"
        android:left="4.5dp"
        android:right="4.5dp"
        android:top="4.5dp">
        <shape android:shape="oval">
            <size
                android:width="@dimen/schedule_check_size"
                android:height="@dimen/schedule_check_size" />
            <solid android:color="?point_2" />
        </shape>
    </item>
</layer-list>